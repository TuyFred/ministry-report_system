# Backend environment variables
PORT=5000

# Secrets (set per environment; do not commit real values)
JWT_SECRET=your_jwt_secret_here
ADMIN_SECRET_KEY=your_admin_secret_here

# Supabase Postgres connection string
# Format: postgresql://postgres:<DB_PASSWORD>@<HOST>:5432/postgres?sslmode=require
DATABASE_URL=postgresql://postgres:YOUR_DB_PASSWORD@YOUR_DB_HOST:5432/postgres?sslmode=require

# Optional defensive setting for pg
PGSSLMODE=require
